<?xml version="1.0" encoding="utf-8"?>
<ProjectSchemaDefinitions xmlns="clr-namespace:Microsoft.Build.Framework.XamlTypes;assembly=Microsoft.Build.Framework" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:sys="clr-namespace:System;assembly=mscorlib" xmlns:transformCallback="Microsoft.Cpp.Dev10.ConvertPropertyCallback">
  <Rule
    Name="arm_linux_gcc"
    PageTemplate="tool"
    DisplayName="GCC Compiler"
    Order="200">
    <Rule.DataSource>
      <DataSource
        Persistence="ProjectFile"
        ItemType="arm_linux_gcc" />
    </Rule.DataSource>
    <Rule.Categories>
      <Category
        Name="General">
        <Category.DisplayName>
          <sys:String>General</sys:String>
        </Category.DisplayName>
      </Category>
      <Category
        Name="Command Line"
        Subtype="CommandLine">
        <Category.DisplayName>
          <sys:String>Command Line</sys:String>
        </Category.DisplayName>
      </Category>
    </Rule.Categories>
    <StringListProperty
      Name="Inputs"
      Category="Command Line"
      IsRequired="true"
      Switch=" ">
      <StringListProperty.DataSource>
        <DataSource
          Persistence="ProjectFile"
          ItemType="arm_linux_gcc"
          SourceType="Item" />
      </StringListProperty.DataSource>
    </StringListProperty>
    <BoolProperty
      Name="WarningsAsErrors"
      HelpContext="0"
      DisplayName="Warnings As Errors"
      Description="Treat warnings as errors"
      Switch="-Werror" />
    <StringListProperty
      Name="IncludePaths"
      HelpContext="0"
      DisplayName="Additional Include Paths"
      Description="Specifies one or more directories to add to the include path; use semi-colon delimited list if more than one."
      Separator=";"
      Switch="-I&quot;[value]&quot;" />
    <StringListProperty
      Name="PreprocessorDefines"
      HelpContext="0"
      DisplayName="Preprocessor Defines"
      Description="Specifies one or more preprocessor defines.  Separate multiple defines with a semi-colon."
      Separator=";"
      Switch="-D[value]" />
    <StringListProperty
      Name="PreprocessorUndefines"
      HelpContext="0"
      DisplayName="Preprocessor Undefines"
      Description="Specifies one or more preprocessor undefs.  Separate multiple undefs with a semi-colon."
      Separator=";"
      Switch="-U[value]" />
    <EnumProperty
      Name="OptimizationLevel"
      HelpContext="0"
      DisplayName="Optimization Level"
      Description="Set the optimization level">
      <EnumValue
        Name="0"
        DisplayName="Default" />
      <EnumValue
        Name="1"
        DisplayName="No Optimization"
        Switch="-O0" />
      <EnumValue
        Name="2"
        DisplayName="Simple Optimization"
        Switch="-O1" />
      <EnumValue
        Name="3"
        DisplayName="Optimize for Speed"
        Switch="-O2" />
      <EnumValue
        Name="4"
        DisplayName="Optimize for Size"
        Switch="-Os" />
      <EnumValue
        Name="5"
        DisplayName="Maximum Optimization"
        Switch="-O3" />
    </EnumProperty>
    <EnumProperty
      Name="WarningLevel"
      HelpContext="0"
      DisplayName="Warning Level"
      Description="Set the level of warnings that are displayed.">
      <EnumValue
        Name="0"
        DisplayName="Default" />
      <EnumValue
        Name="2"
        DisplayName="All"
        Switch="-Wall" />
    </EnumProperty>
    <StringProperty
      Name="CommandLineTemplate"
      DisplayName="Command Line"
      Visible="False"
      IncludeInCommandLine="False" />
    <DynamicEnumProperty
      Name="arm_linux_gccBeforeTargets"
      Category="General"
      EnumProvider="Targets"
      IncludeInCommandLine="False">
      <DynamicEnumProperty.DisplayName>
        <sys:String>Execute Before</sys:String>
      </DynamicEnumProperty.DisplayName>
      <DynamicEnumProperty.Description>
        <sys:String>Specifies the targets for the build customization to run before.</sys:String>
      </DynamicEnumProperty.Description>
      <DynamicEnumProperty.ProviderSettings>
        <NameValuePair
          Name="Exclude"
          Value="^arm_linux_gccBeforeTargets|^Compute" />
      </DynamicEnumProperty.ProviderSettings>
      <DynamicEnumProperty.DataSource>
        <DataSource
          Persistence="ProjectFile"
          HasConfigurationCondition="true" />
      </DynamicEnumProperty.DataSource>
    </DynamicEnumProperty>
    <DynamicEnumProperty
      Name="arm_linux_gccAfterTargets"
      Category="General"
      EnumProvider="Targets"
      IncludeInCommandLine="False">
      <DynamicEnumProperty.DisplayName>
        <sys:String>Execute After</sys:String>
      </DynamicEnumProperty.DisplayName>
      <DynamicEnumProperty.Description>
        <sys:String>Specifies the targets for the build customization to run after.</sys:String>
      </DynamicEnumProperty.Description>
      <DynamicEnumProperty.ProviderSettings>
        <NameValuePair
          Name="Exclude"
          Value="^arm_linux_gccAfterTargets|^Compute" />
      </DynamicEnumProperty.ProviderSettings>
      <DynamicEnumProperty.DataSource>
        <DataSource
          Persistence="ProjectFile"
          ItemType=""
          HasConfigurationCondition="true" />
      </DynamicEnumProperty.DataSource>
    </DynamicEnumProperty>
    <StringListProperty
      Name="Outputs"
      DisplayName="Outputs"
      Visible="False"
      IncludeInCommandLine="False" />
    <StringProperty
      Name="ExecutionDescription"
      DisplayName="Execution Description"
      Visible="False"
      IncludeInCommandLine="False" />
    <StringListProperty
      Name="AdditionalDependencies"
      DisplayName="Additional Dependencies"
      IncludeInCommandLine="False"
      Visible="false" />
    <StringProperty
      Subtype="AdditionalOptions"
      Name="AdditionalOptions"
      Category="Command Line">
      <StringProperty.DisplayName>
        <sys:String>Additional Options</sys:String>
      </StringProperty.DisplayName>
      <StringProperty.Description>
        <sys:String>Additional Options</sys:String>
      </StringProperty.Description>
    </StringProperty>
  </Rule>
  <ItemType
    Name="arm_linux_gcc"
    DisplayName="GCC Compiler" />
  <FileExtension
    Name="*.c;*.cpp"
    ContentType="arm_linux_gcc" />
  <ContentType
    Name="arm_linux_gcc"
    DisplayName="GCC Compiler"
    ItemType="arm_linux_gcc" />
